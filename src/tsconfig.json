{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"jsx": "react",
		"lib": [
			"dom",
			"dom.iterable",
			"es5",
			"es2015.collection",
			"es2015.iterable"
		],
		"module": "es2015",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "../dist",
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2015"
	}
}
